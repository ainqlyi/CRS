      <DTS:Variables />
      <DTS:PropertyExpression
        DTS:Name="SqlStatementSource">"IF OBJECT_ID('"+@[$Project::TempTable]+"', 'U') IS NOT NULL
DROP TABLE "+@[$Project::TempTable]+"
